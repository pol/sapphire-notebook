  "id": "sample1",
  "type": "document",
  "name": "Cell Staining",
  "content": [
    {
      "type":"text",
      "content_type": "text/plain",
      "content": "This is the contexts of a text-block."
    },
    {
      "type": "table",
      "id": "people",
      "content_type": "application/json",
      "content": [
        {"first_name": "John", "last_name": "Smith"},
        {"first_name": "Jane", "last_name": "Doe"}
      ]
    },
    {
      "type": "source",
      "content_type": "application/javascript",
      "content": "alert('some inline code');"
    },
    {
      "type":"form",
      "target": "people",
      "content_type": "application/json+inputex",
      "content": {
        "fields": [
          {"type":"string", "name":"first_name", "label":"First Name"},
          {"type":"string", "name":"last_name", "label":"Last Name"}
        ]
      }
    }
  ]
